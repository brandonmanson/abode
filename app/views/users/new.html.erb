<%= form_for @user do |f| %>
  <%= f.text_field :name, placeholder: "name" %>
  <%= f.email_field :email, placeholder: "email" %>
  <%= f.email_field :phone, placeholder: "phone" %>
  <%= f.password_field :password, placeholder: "password" %>
  <%= f.password_field :password_confirmation, placeholder: "password_confirmation" %>
  <%= f.submit "Sign Up" %>
<% end %>
