<li>
  <div id="expense_<%=expense.id%>" class="expense" >
    Expense:
    <%= link_to "#{expense.name} #{expense.amount}", expense_show_path(expense) %>
    <%= expense.description %>
    payer: <%= expense.payer.name %>
      <div class="owers"><ul>
        OWERS:
        <% expense.users.each do |user| %>
          <li><%=user.name%></li>
        <% end %>
      </ul></div>
  </div>
</li>
